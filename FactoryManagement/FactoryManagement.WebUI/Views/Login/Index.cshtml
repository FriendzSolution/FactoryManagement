
@{
    Layout = null;
    ViewBag.Title = "Login";
}

<!DOCTYPE html>
<html lang="en">


<!-- auth-login.html  21 Nov 2019 03:49:32 GMT -->
<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
    <title>Otika - Admin Dashboard Template</title>
    <link rel="stylesheet" href="~/Content/assets/css/app.min.css">
    <link rel="stylesheet" href="~/Content/assets/bundles/bootstrap-social/bootstrap-social.css">
    <link rel="stylesheet" href="~/Content/assets/css/style.css">
    <link rel="stylesheet" href="~/Content/assets/css/components.css">
    <link rel="stylesheet" href="~/Content/assets/css/custom.css">
    <link rel='shortcut icon' type='image/x-icon' href='~/Content/assets/img/favicon.ico' />
</head>

<body>

    <div id="app">
        <section class="section">
            <div class="container mt-5">
                <div class="row">
                    <div class="col-12 col-sm-8 offset-sm-2 col-md-6 offset-md-3 col-lg-6 offset-lg-3 col-xl-4 offset-xl-4">
                        <div class="card card-primary">
                            <div class="card-header">
                                <h4>Login</h4>
                            </div>
                            <div class="card-body">

                                <div class="form-group">
                                    <label for="email">Username</label>
                                    <input id="username" type="text" class="form-control" name="email" tabindex="1" required autofocus>
                                </div>
                                <div class="form-group">
                                    <label for="email">Password</label>
                                    <input id="password" type="password" class="form-control" name="password" tabindex="2" required>
                                </div>

                                <div class="form-group">
                                    <button type="submit" class="btn btn-primary btn-lg btn-block" tabindex="4" onclick="validate()">
                                        Login
                                    </button>
                                </div>


                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </section>
    </div>
    <script src="~/Content/assets/js/app.min.js"></script>
    <script src="~/Content/assets/js/scripts.js"></script>
    <script src="~/Content/assets/js/custom.js"></script>
    <script src="~/Content/assets/bundles/sweetalert/sweetalert.min.js"></script>
    <script src="~/Content/Pages/Shared/layoutIndex.js"></script>
    <script src="~/Content/Pages/Login/loginIndex.js"></script>

</body>
</html>

